{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error", { "singleQuote": true, "trailingComma": "es5" }],
        "react/prop-types": 0
    },
    "env": {
        "browser": true,
        "node": true
    }
}